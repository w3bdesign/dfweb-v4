{
  "name": "dfweb-v4",
  "version": "1.0.8",
  "private": true,
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "dev": "next dev --turbopack",
    "lhci": "lhci autorun",
    "lhci:perf": "lhci autorun --collect.settings.preset=perf",
    "lhci:desktop": "lhci autorun --collect.settings.preset=desktop",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "test": "NODE_OPTIONS='--no-experimental-strip-types' jest --coverage --collectCoverageFrom='src/components/**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='!src/components/Animations/Matrix.component.{js,jsx,ts,tsx}' --collectCoverageFrom='!src/components/Animations/types/**'",
    "test:watch": "NODE_OPTIONS='--no-experimental-strip-types' jest --watch",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-test dev 3000 cypress:headless",
    "refresh": "pnpm i && rm -rf node_modules && rm pnpm-lock.yaml && pnpm store prune && pnpm i && pnpm format",
    "ladle": "ladle serve",
    "typegen": "npx sanity typegen generate",
    "doctor": "react-doctor . --verbose"
  },
  "dependencies": {
    "@babel/helpers": ">=7.28.6",
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.2",
