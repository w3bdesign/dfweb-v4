    "typescript": "^5.9.3",
    "wait-on": "^9.0.4"
  },
  "pnpm": {
    "overrides": {
      "@pnpm/npm-conf": ">=3.0.2",
      "minimatch": ">=10.2.3",
      "rollup": ">=4.59.0"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "cypress",
      "esbuild",
      "msw",
      "puppeteer",
      "sharp",
      "unrs-resolver"
    ]
  }
