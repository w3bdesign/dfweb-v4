    "@vercel/speed-insights": "1.3.1",
    "autoprefixer": "^10.4.24",
    "babel-jest": "30.2.0",
    "clsx": "^2.1.1",
    "cookie": ">=1.1.1",
    "envalid": "^8.1.1",
    "jest": "30.2.0",
    "motion": "^12.34.0",
    "next": "^16.1.6",
    "next-sanity": "^12.1.0",
    "path-to-regexp": "^8.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.71.1",
    "react-icons": "^5.5.0",
    "sanity": "5.9.0",
    "sitemap": "^9.0.0",
    "tar-fs": "^3.1.1",
    "ts-node": "^10.9.2",
    "zod": "^4.3.6"
  },
